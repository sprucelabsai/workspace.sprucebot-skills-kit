{
	"name": "@sprucelabs/spruce-next-helpers",
	"publishConfig": {
		"access": "public"
	},
	"version": "8.17.7",
	"main": "lib/index.js",
	"description": "Spruce Next.js helpers and utitlies",
	"keywords": [
		"react",
		"node",
		"components",
		"sprucebot",
		"sprucelabs"
	],
	"license": "SEE LICENCE in LICENCE",
	"homepage": "https://github.com/sprucelabsai/workspace.sprucebot-skills-kit",
	"bugs": {
		"url": "https://github.com/sprucelabsai/workspace.sprucebot-skills-kit/issues"
	},
	"scripts": {
		"build": "babel --source-maps --extensions \".js\",\".tsx\",\".ts\" ./src --out-dir ./lib && tsc --declaration --emitDeclarationOnly --declarationMap",
		"clean": "rm -rf lib/*",
		"lint": "eslint --max-warnings=0 --ext=.js,.jsx,.ts,.tsx src && tsc -p . --noEmit",
		"#test": "jest",
		"watch": "chokidar 'src/**/*.(js|jsx|ts|tsx)' -c 'npm run build'"
	},
	"devDependencies": {
		"@babel/core": "^7.1.6",
		"@babel/preset-flow": "^7.0.0",
		"@babel/runtime": "^7.4.5",
		"babel-loader": "^8.0.6",
		"babel-plugin-inline-react-svg": "^1.0.1",
		"chokidar-cli": "^1.2.0",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"enzyme-to-json": "^3.3.1",
		"eslint": "^5.9.0",
		"eslint-config-spruce": "^8.17.7",
		"jest": "^24.5.0",
		"prettier": "^1.13",
		"react-test-renderer": "^16.0.0"
	},
	"dependencies": {
		"@babel/polyfill": "^7.0.0",
		"@babel/register": "^7.0.0",
		"@sprucelabs/heartwood-components": "^8.17.7",
		"@sprucelabs/react-heartwood-components": "^8.17.7",
		"@sprucelabs/spruce-types": "^8.17.7",
		"@types/dinero.js": "^1.4.1",
		"@zeit/next-typescript": "^1.1.1",
		"axios": "^0.18.0",
		"classnames": "^2.2.5",
		"cookies": "^0.7.3",
		"debug": "^3.1.0",
		"dinero.js": "^1.6.0",
		"is_js": "^0.9.0",
		"js-cookies": "^1.0.4",
		"moment": "^2.24.0",
		"next": "^7.0.0",
		"next-redux-wrapper": "^1.0.0",
		"prop-types": "^15.6.0",
		"qs": "^6.5.1",
		"react": "^16.3.2",
		"redux": "^3.7.2",
		"redux-form": "^7.3.0"
	},
	"jest": {
		"roots": [
			"<rootDir>/src/"
		],
		"coverageDirectory": "./coverage/",
		"collectCoverage": true,
		"coveragePathIgnorePatterns": [
			"<rootDir>/lib/",
			"<rootDir>/node_modules/"
		]
	}
}
