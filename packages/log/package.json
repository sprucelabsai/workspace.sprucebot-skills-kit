{
	"name": "@sprucelabs/log",
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"build"
	],
	"version": "9.1.0",
	"main": "./build/index.js",
	"types": "./build/index.d.ts",
	"description": "Logger built for the Spruce Experience Platform",
	"keywords": [
		"node",
		"log",
		"logger",
		"sprucebot",
		"sprucelabs"
	],
	"homepage": "https://github.com/sprucelabsai/workspace.sprucebot-skills-kit",
	"bugs": {
		"url": "https://github.com/sprucelabsai/workspace.sprucebot-skills-kit/issues"
	},
	"scripts": {
		"build": "npm run clean && concurrently \"npm run build:node\" \"npm run build:umd\"",
		"build:node": "tsc | true",
		"build:browser": "tsc --project tsconfig.browser.json",
		"build:umd": "rollup -c rollup.config.js",
		"watch": "chokidar 'src/**/*.(js|jsx|ts|tsx)' -c 'npm run build'",
		"clean": "rm -rf build/",
		"lint": "eslint '**/*.ts' && tsc -p . --noEmit",
		"lint:fix": "eslint --fix '**/*.ts'"
	},
	"dependencies": {},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^11.0.0",
		"@rollup/plugin-json": "^4.0.1",
		"@rollup/plugin-node-resolve": "^6.0.0",
		"@rollup/plugin-typescript": "^2.0.1",
		"@types/chai": "^4.2.3",
		"@types/mocha": "^5.2.7",
		"chai": "^4.2.0",
		"chokidar-cli": "^2.1.0",
		"concurrently": "^5.0.2",
		"eslint": "^5",
		"eslint-config-spruce": "^9.0.0",
		"faker": "^4.1.0",
		"mocha": "^5.2.0",
		"prettier": "^1.19",
		"rollup": "^1.27.14",
		"rollup-plugin-node-builtins": "^2.1.2",
		"rollup-plugin-node-globals": "^1.4.0",
		"rollup-plugin-replace": "^2.2.0",
		"typescript": "^3.8"
	}
}
