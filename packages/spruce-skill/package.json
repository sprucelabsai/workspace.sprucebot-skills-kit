{
	"name": "@sprucelabs/spruce-skill",
	"publishConfig": {
		"access": "public"
	},
	"description": "The base template that drives your Sprucebot Skill! ‚öôÔ∏èüå≤ü§ñ",
	"version": "8.6.2",
	"homepage": "https://github.com/sprucelabsai/workspace.sprucebot-skills-kit",
	"bugs": {
		"url": "https://github.com/sprucelabsai/workspace.sprucebot-skills-kit/issues"
	},
	"keywords": [
		"skills-kit",
		"sprucebot",
		"sprucelabs"
	],
	"engines": {
		"node": "^10",
		"npm": "^6"
	},
	"scripts": {
		"build": "npm run build:strip-flow && next build interface",
		"build:strip-flow": "rm -rf build/ && flow-remove-types server/ --out-dir build/",
		"start": "node ./build/server.js",
		"local": "nodemon ./server/server.js --exec \"npm run kill-inspect && flow-node --inspect=4006\" --watch server --watch config --verbose --ignore server/swagger --ignore server/static",
		"kill-inspect": "kill-port 4006",
		"interface": "next ./interface -p 3006",
		"lint": "./node_modules/.bin/eslint '**/**/*{.js}'",
		"test": "yarn run build && jest",
		"test:server": "PORT=3010 BLUEBIRD_W_FORGOTTEN_RETURN=0 ALLOW_NON_SSL=true ORM_LOGGING=false TESTING=true mocha --exit 'server/tests/**/*Tests.js'",
		"test:server:single": "PORT=3010 BLUEBIRD_W_FORGOTTEN_RETURN=0 ALLOW_NON_SSL=true ORM_LOGGING=false TESTING=true mocha --exit",
		"icon": "./node_modules/.bin/svgo icon/icon.svg",
		"precommit": "npm run lint",
		"prepush": "npm run lint && npm run test",
		"heroku-postbuild": "npm run build",
		"#postinstall": "npm run build",
		"release": "semantic-release"
	},
	"dependencies": {
		"@babel/runtime": "^7.0.0",
		"@sprucelabs/heartwood-components": "^8.6.2",
