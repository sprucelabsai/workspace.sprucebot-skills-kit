	"scripts": {
		"build": "npm run clean && npm run build:server && npm run build:config && npm run build:interface",
		"build:interface": "NODE_CONFIG_DIR=./build/config next build interface",
		"build:server": "tsc --project tsconfig.server.json | true && npm run build:copy-gql",
		"build:config": "cp -r icon build && NODE_CONFIG_DIR=./build/config node ./bin/buildConfigs.js",
		"build:copy-gql": "copyfiles -V ./server/**/*.gql ./server/**/*.graphql build/",
		"clean": "rm -rf build/",
		"start": "NODE_CONFIG_DIR=./build/config node ./build/server/server.js",
		"local": "npm run kill-inspect && tsnd --ignore-watch interface/.next --ignore-watch node_modules  --ignore-watch generated.ts --project tsconfig.server.json --inspect=4080 --respawn --transpileOnly server/server.ts",
		"kill-inspect": "kill-port 4080",
		"interface": "next ./interface -p 3080",
		"lint": "eslint --max-warnings=0 --ext=.js,.jsx,.ts,.tsx . && tsc -p . --noEmit",
		"tsc": "tsc",
		"test.old": "yarn run build && jest",
		"test": "rm tmp/testing.db | true && mkdir -p tmp/ | true && TS_NODE_PROJECT=tsconfig.server.json PORT=3010 BLUEBIRD_W_FORGOTTEN_RETURN=0 ALLOW_NON_SSL=true ORM_LOGGING=false TESTING=true API_ONLY=true DB_MIGRATIONS=false mocha -r ts-node/register/transpile-only --exit 'server/tests/**/*Tests*' --inspect=4080",
		"test:server": "rm tmp/testing.db | true && mkdir -p tmp/ | true && TS_NODE_PROJECT=tsconfig.server.json PORT=3010 BLUEBIRD_W_FORGOTTEN_RETURN=0 ALLOW_NON_SSL=true ORM_LOGGING=false TESTING=true API_ONLY=true DB_MIGRATIONS=false mocha -r ts-node/register/transpile-only --exit 'server/tests/**/*Tests*' --inspect=4080",
		"test:server:single": "rm tmp/testing.db | true && mkdir -p tmp/ | true && TS_NODE_PROJECT=tsconfig.server.json PORT=3010 BLUEBIRD_W_FORGOTTEN_RETURN=0 ALLOW_NON_SSL=true ORM_LOGGING=false TESTING=true API_ONLY=true DB_MIGRATIONS=false mocha -r ts-node/register/transpile-only --exit",
		"icon": "svgo icon/icon.svg",
		"heroku-postbuild": "npm run build",
		"release": "semantic-release",
		"storybook": "start-storybook -p 6006",
		"gqlToTS": "graphql-codegen -r dotenv/config --config codegen.yml && prettier-eslint -l info --write \"server/interfaces/gql.ts\"",
		"checkIfGitIsClean": "! git status --short | grep ."
	},
