		"@zeit/next-sass": "^1.0.1",
		"babel-core": "^7.0.0-bridge.0",
		"babel-plugin-transform-flow-strip-types": "^6.22.0",
		"config": "^1.27.0",
		"dotenv": "^5.0.1",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"enzyme-to-json": "^3.3.1",
