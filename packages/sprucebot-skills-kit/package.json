		"enzyme-to-json": "^3.3.1",
		"eslint-plugin-react": "^7.4.0",
		"jsonwebtoken": "^8.1.0",
		"lodash": "^4.17.4",
		"moment-timezone": "^0.5.14",
		"next": "^5.1.0",
		"prop-types": "^15.6.0",
		"react": "^16.0.0",
		"react-dom": "^16.0.0",
		"react-emojione": "^5.0.0",
		"react-redux": "^5.0.6",
		"redux": "^3.7.2",
		"styled-components": "^3.2.1",
		"universal-cookie": "^2.1.0"
	},
	"devDependencies": {
		"babel-jest": "^22.0.4",
