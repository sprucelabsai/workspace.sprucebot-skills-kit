{
	"name": "@sprucelabs/sprucebot-skills-kit",
	"publishConfig": {
		"access": "public"
	},
	"description": "The base template that drives your Sprucebot Skill! ‚öôÔ∏èüå≤ü§ñ",
	"version": "6.56.0",
	"homepage": "https://github.com/sprucelabsai/sprucebot-skills-kit",
	"bugs": {
		"url": "https://github.com/sprucelabsai/sprucebot-skills-kit/issues"
	},
	"keywords": [
		"skills-kit",
		"sprucebot",
		"sprucelabs"
	],
	"engines": {
		"node": "^8.5.0",
		"npm": "^5.4.2"
	},
	"scripts": {
		"build": "next build interface",
		"start": "node ./server/server.js",
		"local": "nodemon server/server.js --exec \"node --inspect=4006\" --watch server --watch config --verbose --ignore server/swagger --ignore server/static",
		"interface": "next ./interface -p 3006",
		"lint": "./node_modules/.bin/eslint '**/**/*{.js}'",
		"test": "yarn run build && jest --clearCache && jest",
		"test:server": "PORT=3010 BLUEBIRD_W_FORGOTTEN_RETURN=0 ALLOW_NON_SSL=true ORM_LOGGING=false TESTING=true mocha --exit 'server/tests/**/*Tests.js'",
		"test:server:single": "PORT=3010 BLUEBIRD_W_FORGOTTEN_RETURN=0 ALLOW_NON_SSL=true ORM_LOGGING=false TESTING=true mocha --exit",
		"icon": "./node_modules/.bin/svgo icon/icon.svg",
		"precommit": "npm run lint",
		"prepush": "npm run lint && npm run test",
		"heroku-postbuild": "npm run build",
		"postinstall": "npm run build",
		"release": "semantic-release"
	},
	"dependencies": {
		"@barbershopio/iso-log": "^0.1.13",
		"@sprucelabs/react-sprucebot": "^6.52.0",
