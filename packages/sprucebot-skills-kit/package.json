		"husky": "^0.14.3",
		"jest": "^22.0.5",
		"jest-cli": "^22.0.5",
		"jest-styled-components": "^5.0.0",
		"nock": "^9.2.3",
		"nodemon": "^1.18.3",
		"prettier": "^1.13",
		"raf": "^3.4.0",
		"svgo": "^1.0.3",
		"swagger-jsdoc": "^3.0.2"
	},
	"jest": {
		"coverageDirectory": "./coverage/",
		"collectCoverage": true
	}
