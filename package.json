{
    "name": "sprucebot-skills-kit-server",
    "version": "0.0.7",
    "scripts": {
        "lint": "./node_modules/.bin/eslint --fix '**/**/*{.js,.scss}'",
        "test": "jest --coverage",
        "precommit": "npm run lint",
        "prepush": "npm run lint && npm run test"
    },
    "dependencies": {
        "@koa/cors": "2",
        "config": "^1.28.0",
        "cookies": "^0.7.1",
        "glob": "^7.1.2",
        "jsonwebtoken": "^8.1.0",
        "koa": "^2.4.1",
        "koa-bodyparser": "^4.2.0",
        "koa-router": "^7.2.1",
        "koa-static": "^4.0.1",
        "next": "^4.1.4",
        "node-cron": "^1.2.1",
        "react": "^16.0.0",
        "react-dom": "^16.0.0"
    },
    "devDependencies": {
        "add": "^2.0.6",
        "eslint": "^4.10.0",
        "eslint-config-prettier": "^2.6.0",
        "eslint-plugin-prettier": "^2.3.1",
        "jest": "^21.2.1",
        "prettier": "^1.7.4",
        "yarn": "^1.2.1"
    },
    "jest": {
        "clearMocks": true,
        "testPathIgnorePatterns": [
            "/config/"
        ],
        "collectCoverageFrom": [
            "**/*.js",
            "!**/node_modules/**",
            "!**/__TEST__/**"
        ],
        "coverageDirectory": "coverage"
    }
}
